{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {
        "url": "https://raw.githubusercontent.com/2XUID/A2/master/data/complete.csv"
    },
    "width": 900,
    "height": 200,
    "title": {
        "text": "Number of sightings vs Time",
        "fontSize": 18
    },
    "mark": "line",
    "encoding": {
        "tooltip": [
            {
                "timeUnit": "utchours",
                "field": "datetime",
                "type": "temporal",
                "title": "Time"
            },
            {
                "field": "record",
                "type": "quantitative",
                "aggregate": "count"
            }
        ],
        "x": {
            "timeUnit": "utchours",
            "field": "datetime",
            "type": "temporal",
            "title": "Time"
        },
        "y": {
            "field": "record",
            "type": "quantitative",
            "aggregate": "count"
        }
    }
}