    {
        "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
        "description": "A simple donut chart with embedded data.",
        "data": {
            "url": "https://raw.githubusercontent.com/2XUID/A2/master/data/country.csv",
            "format": {
                "type": "csv"
            }
        },
        "mark": {
            "type": "arc",
            "innerRadius": 50
        },
        "encoding": {
            "theta": {
                "field": "count",
                "type": "quantitative"
            },
            "color": {
                "field": "country",
                "type": "nominal"
            }
        }
    }